program wb_test6
  implicit none
#include "WhiteBoard.hf"
  character(len=5),parameter :: OK = 'OK   '
  character(len=5),parameter :: FAIL = 'FAIL '
  integer :: values(2),istat,n
  character(len=64) :: fnname,package,myfile,mysect
  !---------------------------------------------------------------------
  istat = wb_verbosity(WB_MSG_DEBUG)

  fnname = 'sample6'
  package = trim(fnname)//'/'
  myfile  = trim(fnname)//'.dict'
  mysect  = trim(fnname)//'_sec'
  istat = wb_read(package,myfile,mysect,WB_STRICT_DICTIONARY)
  if (WB_IS_OK(istat)) then
     print *,OK,trim(fnname)
  else
     print *,FAIL,trim(fnname)
  endif

  istat = wb_get(trim(package)//'a',values,n)
  if (WB_IS_OK(istat)) then
     print *,OK,trim(fnname),values
  else
     print *,FAIL,trim(fnname)
  endif

end program wb_test6
