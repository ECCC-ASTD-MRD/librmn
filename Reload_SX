make ARCH=SX4 ABI=_64 genlib
make ARCH=SX4 ABI=_64_float1 clean
make ARCH=SX4 ABI=_64_float1 genlib
make ARCH=SX4 ABI=_64_multi clean
make ARCH=SX4 ABI=_64_multi genlib
make ARCH=SX4 ABI=_64_multi_float1 clean
make ARCH=SX4 ABI=_64_multi_float1 genlib
make ARCH=SX4 ABI=_float1 clean
make ARCH=SX4 ABI=_float1 genlib
make ARCH=SX4 ABI=_multi clean
make ARCH=SX4 ABI=_multi genlib
make ARCH=SX4 ABI=_multi_float1 clean
make ARCH=SX4 ABI=_multi_float1 genlib
make ARCH=SX5 clean
make ARCH=SX5 genlib
make ARCH=SX5 ABI=_64 clean
make ARCH=SX5 ABI=_64 genlib
make ARCH=SX5 ABI=_64_multi clean
make ARCH=SX5 ABI=_64_multi genlib
make ARCH=SX5 ABI=_multi clean
