*C     TERMINE LA BOUCLE
DEFINE(I,1)
DEFINE(TABL,4)
TABL=12,18,24,30
I=0
I=(I+2)
PRINT(I,'(T35,I5.5)')
while ((I:=I+1) <= 4)
PRINT(I,'(T35,I5.5)')
PRINT(TABL[I],'(T35,I5.5)')
endwhile

